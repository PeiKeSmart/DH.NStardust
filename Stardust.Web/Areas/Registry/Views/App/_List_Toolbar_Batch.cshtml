@using NewLife.Common;
@using System.Collections.Generic;
@{
    var set = ViewBag.PageSetting as PageSetting;
}
@if (set.EnableSelect)
{
    <button type="button" class="btn btn-success btn-sm" data-action="action" data-url="@Url.Action("SetAlarm")?enable=true" data-fields="keys" disabled>
        启用告警
    </button>
    <button type="button" class="btn btn-error btn-sm" data-action="action" data-url="@Url.Action("SetAlarm")?enable=false" data-fields="keys" disabled>
        禁用告警
    </button>
}