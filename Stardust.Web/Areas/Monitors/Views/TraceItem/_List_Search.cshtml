@using NewLife;
@using NewLife.Web;
@using NewLife.Cube;
@using XCode;
@using Stardust.Data.Monitors;
@{
    var fact = ViewBag.Factory as IEntityFactory;
    var page = ViewBag.Page as Pager;
    var dic = AppTracer.FindAllWithCache().ToDictionary(e => e.ID, e => e + "");
}
<div class="form-group">
    <label for="appId" class="control-label">应用：</label>
    @Html.ForDropDownList("appId", dic, page["appId"], "全部", true)
</div>
<div class="form-group">
    <label for="kind" class="control-label">类别：</label>
    @Html.ForDropDownList("kind", TraceItem.GetKinds(), page["kind"], "全部", true)
</div>
@await Html.PartialAsync("_Enable", "")
@await Html.PartialAsync("_DateRange")